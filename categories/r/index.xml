&lt;h2 id=&#34;un-ggplot-clasico&#34;&gt;un ggplot clasico&lt;/h2&gt;

&lt;pre&gt;&lt;code class=&#34;language-r&#34;&gt;library(ggplot2)
&lt;/code&gt;&lt;/pre&gt;

&lt;pre&gt;&lt;code class=&#34;language-r&#34;&gt;ggplot(diamonds) +
  geom_jitter(aes(x = carat, y = cut, color = price)) +
  scale_color_viridis_c(option = &amp;quot;inferno&amp;quot;)
&lt;/code&gt;&lt;/pre&gt;

&lt;p&gt;&lt;img src=&#34;/post/2019-10-18-probando-nomas_files/figure-html/unnamed-chunk-2-1.png&#34; width=&#34;672&#34; /&gt;&lt;/p&gt;

&lt;h1 id=&#34;y-otro-con-fondo-blanco&#34;&gt;y otro con fondo blanco&lt;/h1&gt;

&lt;pre&gt;&lt;code class=&#34;language-r&#34;&gt;ggplot(diamonds) +
  geom_jitter(aes(x = carat, y = cut, color = price)) +
  scale_color_viridis_c(option = &amp;quot;inferno&amp;quot;) +
  theme_void()
&lt;/code&gt;&lt;/pre&gt;

&lt;p&gt;&lt;img src=&#34;/post/2019-10-18-probando-nomas_files/figure-html/unnamed-chunk-3-1.png&#34; width=&#34;672&#34; /&gt;&lt;/p&gt;
